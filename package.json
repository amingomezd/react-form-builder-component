{
  "name": "react-form-builder-component",
  "version": "1.0.5",
  "description": "Form Builder package from json",
  "author": "ktmcodelabs",
  "license": "MIT",
  "repository": "ktmcodelabs/react-form-builder-component",
  "homepage": "https://ktmcodelabs.github.io/react-form-builder-component/",
  "main": "./dist/index.js",
  "module": "./dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
