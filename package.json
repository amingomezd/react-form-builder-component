  "bugs": {
    "url": "https://github.com/ktmcodelabs/react-form-builder-component/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.5",
    "@reduxjs/toolkit": "^1.6.2",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
